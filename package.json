{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/drivers/express/bin/www.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm migration:generate -- --name",
    "typeorm:migration:create": "npm run typeorm migration:create -- --name",
    "typeorm:migration:run": "npm run typeorm migration:run",
    "typeorm:migration:revert": "npm run typeorm migration:revert",
    "typeorm:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js",
    "typeorm:seed:config": "npm run typeorm:seed config",
    "typeorm:seed:run": "npm run typeorm:seed seed",
    "typeorm:schema": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:schema:drop": "npm run typeorm:schema schema:drop",
    "typeorm:schema:sync": "npm run typeorm:schema schema:sync",
    "test": "jest",
    "docker:build": "docker-compose build --no-cache",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:rmi": "docker rmi -f investment-support-api:express investment-support-api:mysql5.7"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.5",
    "debug": "~4.3.1",
    "express": "~4.17.1",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typeorm": "^0.2.32",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1"
  }
}
